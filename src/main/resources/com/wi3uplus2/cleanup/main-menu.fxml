      </ImageView>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" spacing="20.0">
        <padding>
          <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
      
        <Label fx:id="welcomeText" text="GAME CLEAN UP!!!">
            <font>
               <Font size="36.0" />
            </font>
         </Label>
        <Button fx:id="startButton" onAction="#onStartButtonClick" text="START">
            <font>
               <Font size="36.0" />
            </font>
         </Button>
      </VBox>
   </children>
</Pane>
